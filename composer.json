{
    "name": "i.janjua/php",
    "autoload": {
        "psr-4": {
            "IJanjua\\Php\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Imam Janjua",
            "email": "i.janjua@simplicity.ag"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1"
    },
    "scripts": {
        "test": "vendor/bin/phpunit test",
        "analyse": "vendor/bin/phpstan analyse -l 9 src",
        "lint-dry": "vendor/bin/php-cs-fixer fix --dry-run --diff src",
        "lint": "vendor/bin/php-cs-fixer --diff src "
    }
}
